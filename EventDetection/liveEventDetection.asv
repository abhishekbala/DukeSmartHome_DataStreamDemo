function liveEventDetection()
    while 1
    liveData = csvread('../dataCollectors/shData.csv');
    times = liveData(100:length(liveData(:,1)),1);
    aggregatePower = sum(liveData(1500:length(liveData(:,1)),2:3),2);
    
    refresh
    % Original data
    figure(1);
    plot(times,aggregatePower);
    % Event Detection
    pause(1)
    end
end